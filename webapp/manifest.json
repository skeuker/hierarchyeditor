{
	"_version": "1.8.0",
	"sap.app": {
		"id": "pnp.hierarchyeditor",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"CA_HIERARCHY_EDITOR_SRV": {
				"uri": "/sap/opu/odata/pnp/CA_HIERARCHY_EDITOR_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/CA_HIERARCHY_EDITOR_SRV/metadata.xml"
				}
			},
			"CA_SERV_HIERARCHY_SRV": {
				"uri": "/sap/opu/odata/pnp/CA_SERV_HIERARCHY_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/CA_SERV_HIERARCHY_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "pnp.hierarchyeditor.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pnp.hierarchyeditor.i18n.i18n"
				}
			},
			"HierarchyModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "Request"
				},
				"dataSource": "CA_HIERARCHY_EDITOR_SRV",
				"preload": true
			},
			"ServiceModel": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline"
				},
				"dataSource": "CA_SERV_HIERARCHY_SRV",
				"preload": true
			},
			"OptionsModel": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/OptionsModel.json"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "pnp.hierarchyeditor.view",
				"controlId": "layout",
				"controlAggregation": "beginColumnPages",
				"bypassed": {
					"target": "notFound"
				},
				"clearControlAggregation": false
			},
			"routes": [
				{
					"pattern": "",
					"name": "App",
					"target": [
						"Selector",
						"Hierarchy",
						"Attributes"
					]
				}
			],
			"targets": {
				"Selector": {
					"viewLevel": 1,
					"viewType": "XML",
					"viewId": "Selector",
					"viewName": "Selector",
					"controlAggregation": "beginColumnPages"
				},
				"Hierarchy": {
					"viewLevel": 1,
					"viewType": "XML",
					"viewId": "Hierarchy",
					"viewName": "Hierarchy",
					"controlAggregation": "midColumnPages"
				},
				"Attributes": {
					"viewLevel": 1,
					"viewType": "XML",
					"viewId": "Attributes",
					"viewName": "Attributes",
					"controlAggregation": "endColumnPages"
				}
			}
		}
	}
}